#include "header.h"
#include "declarations.h"

struct skull_dev *ldev;

int dev_open(struct inode *inodep,struct file *filep)
{   
    printk(KERN_INFO "Inside dev_open");
    ldev=dev;
    ldev=container_of(inodep->i_cdev,struct skull_dev,cdev);
    if(!ldev)
    {
         printk(KERN_ERROR "ldev_not valid returnin dev_open");
    }
    filep->private_data=ldev;
    printk(KERN_INFO "Leaving dev_open");
     return 0;
}
